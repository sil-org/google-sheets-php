{
    "name": "silinternational/google-sheets-php",
    "description": "A simple library for pushing data to a Google Sheet using the Google PHP API Client version 1",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4||^8.1",
        "ext-json": "*",
        "google/apiclient": "^v1.1.9",
        "google/apiclient-services": "^0.176"
    },
    "require-dev": {
        "behat/behat": "^3.6",
        "roave/security-advisories": "dev-master",
        "silinternational/php-env": "^2.1||^3.0"
    },
    "autoload": {
        "psr-4": {
            "Sil\\GoogleSheets\\": "./src",
            "Sil\\GoogleSheets\\features\\": "./features"
        }
    }
}
